<div class="container">
  <div class="panel panel-success">
    <div class="panel-heading">
      <h3 class="panel-title">New Meme</h3>
    </div>
    <%= simple_form_for(@meme) do |f| %>
      <div class="panel-body">
        <%= f.input :category, as: :select, collection: ['succes baby', 'fry'], :prompt => 'Select Category' %><br><br>
        <%= f.input :photo %><br>
        <%= f.submit class: "btn btn-success"%>
      </div>
    <% end %>
  </div>
</div>
